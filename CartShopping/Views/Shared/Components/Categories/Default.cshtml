@model IEnumerable<Category>
<ul class ="list-group">
    <li class="list-group-items">
       <a href="" class="nav-link text-dark" asp-controller ="Products" asp-action="Index">All Products</a>
    </li>

    @foreach(var item in Model)
    {
     <li class="list-group-items">
       <a href="" class="nav-link text-dark" asp-controller ="Products" asp-action="Index">@item.Name</a>
    </li>   
    }

</ul>